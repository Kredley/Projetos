package br.com.provaEntrevista;

public class interacao {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		switch(2){
		
		case 1: 
			System.out.println("a");
			break;
		case 2:
			System.out.println("b");
		case 3:
			System.out.println("c");
			
		default:
			System.out.println("erro");

		case 4:
			System.out.println("d");
		}
		
		
			
		
	}

}
